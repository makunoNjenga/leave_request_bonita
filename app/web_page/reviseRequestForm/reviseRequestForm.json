      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.request ){",
        "\treturn {",
        "\t\t//map request variable to expected task contract input",
        "\t\trequestInput: {",
        "\t\t\tdescription: $data.request.description !== undefined ? $data.request.description : null,",
        "\t\t\tstartDate: $data.request.startDate !== undefined ? $data.request.startDate : null,",
        "\t\t\tendDate: $data.request.endDate !== undefined ? $data.request.endDate : null,",
        "\t\t\tcancelApplication: $data.request.cancelApplication !== undefined ? $data.request.cancelApplication : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
